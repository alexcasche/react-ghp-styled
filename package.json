{
  "name": "react-sidebar",
  "version": "0.0.0",
  "description": "Flexible React Sidebar Component",
  "author": "alexcasche",
  "license": "MIT",
  "repository": "alexcasche/react-sidebar",
  "homepage": "https://github.com/alexcasche/react-sidebar#readme",
  "keywords": [
    "react",
    "component",
    "sidebar",
    "menu",
    "collapse",
    "flexible"
  ],
  "main": "index.js",
  "module": "index.es.js",
  "jsnext:main": "index.es.js",
  "scripts": {
    "test": "cross-env CI=1 react-scripts test --env=jsdom",
    "rename": "mv index.css style.css",
    "clean": "rm index.es.css index.es.js",
    "copy": "cp style.css example/src/",
    "build": "rollup -c && yarn run rename && yarn run clean && yarn run copy",
    "start": "yarn run build && rollup -c -w",
    "prepare": "yarn run build && yarn run clean",
    "predeploy": "cd example && yarn install && yarn run build",
    "deploy": "gh-pages -d example/build"
  },
  "peerDependencies": {
    "prop-types": "^15.5.4",
    "react": "^15.0.0 || ^16.0.0",
    "react-dom": "^15.0.0 || ^16.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "concurrently": "^3.5.1",
    "cross-env": "^5.1.4",
    "cssnano": "^3.10.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-config-standard-react": "^6.0.0",
    "eslint-plugin-react": "^7.7.0",
    "eslint-plugin-standard": "^3.0.1",
    "gh-pages": "^1.1.0",
    "node-sass": "^4.9.0",
    "node-sass-chokidar": "^1.2.2",
    "nodemon": "^1.17.3",
    "postcss-cli": "^5.0.0",
    "postcss-cssnext": "^3.1.0",
    "postcss-nested": "^3.0.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-scripts": "^1.1.1",
    "rollup": "^0.54.0",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-commonjs": "^8.2.1",
    "rollup-plugin-node-resolve": "^3.0.2",
    "rollup-plugin-peer-deps-external": "^2.0.0",
    "rollup-plugin-postcss": "^1.6.1",
    "rollup-plugin-scss": "^0.4.0",
    "rollup-plugin-url": "^1.3.0",
    "shelljs": "^0.8.1"
  },
  "dependencies": {
    "classnames": "^2.2.5"
  },
  "files": [
    "index.js",
    "index.es.js",
    "style.css",
    "style.es.css"
  ]
}
